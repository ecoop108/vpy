{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "vpy",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/vpy"
            },
            "args": [
                "-i",
                "examples/name.py",
                "-t",
                "init",
            ],
            "justMyCode": false
        },
        {
            "name": "Python: Current file",
            "type": "python",
            "request": "launch",
            "module": "vpy",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/vpy"
            },
            "args": [
                "-i",
                "examples/tchecker.py",
                "-t",
                "init",
            ],
            "justMyCode": false
        },
        {
            "name": "Python: Module with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/vpy"
            },
            "args": [
                "-o",
                "tmp.prof",
                "-m",
                "vpy",
                "-i",
                "examples/api.py",
                "-t",
                "v5",
            ]
        }
    ]
}