    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            // "module": "examples/name.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/vpy"
            },
            "program": "examples/name.py",
            // "args": [
            // "-i",
            // "examples/name.py",
            // "-t",
            // "init",
            // ],
            "justMyCode": false
        },
        {
            "name": "Python: Module with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/vpy"
            },
            "args": [
                "-o",
                "tmp.prof",
                "-m",
                "vpy",
                "-i",
                "examples/api.py",
                "-t",
                "v5",
            ]
        }
    ]
}